ç語言
循環執行
==============================================
#include <stdio.h>
 
int main ()
{
   
   for( int a = 10; a < 20; a = a + 1 )
   {
      printf("a 的值： %d\n", a);
   }
 
   return 0;
}

=============================================
C ++
如果符合就執行這回圈，否則就跳出這迴圈
=============================================
#include <iostream>
using namespace std;
 
int main ()
{
   
   for( int a = 10; a < 20; a = a + 1 )
   {
       cout << "a 的值：" << a << endl;
   }
 
   return 0;
}
===========================================

java
============================================
public class Test {
   public static void main(String args[]) {
 
      for(int x = 10; x < 20; x = x+1) {
         System.out.print("value of x : " + x );
         System.out.print("\n");
      }
   }
}
=============================================






