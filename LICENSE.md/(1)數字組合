的#include <stdio.h>中

int main（）{int i，j，k;
     printf（“\ n”）;
     for（i = 1; i <5; i ++）{//以下為三重循環 

       
        （k = 1; k <5; k ++）{//確保i，j，k三位互不相同 
               
                如果（i！= k && i！= j && j！= k）{printf（“％d，％d，％d \ n”，i，j，k）;
                 }}}}}   
