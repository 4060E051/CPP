c語言 取商數 取餘數
=====================================================
#include <stdio.h>
#include<stdlib.h>

int main(void){
int a;
int b;

a=3/2;
b=3%2;

printf("取商數:%d\n",a);
printf("取餘數:%d\n",b);

system("pause");
return 0;


}

======================================================

c++  取商數 取餘數
==============================================================

#include "iostream"


using namespace std;


int main()<font></font>
{<font></font>

    int a=49,b=10,c; 
    
    c=a/b;
    <font></font>
    cout<<c;
    return 0;
}



