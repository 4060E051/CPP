#include <iostream>
 
using namespace std;
 
class as {
public:
    int a;
    int b;
    int do_something();
};
 
int as::do_something() {
    return a + b;
}
 
int main(void) {
    as t;
    t.a = 11;
    t.b = 22;
  
    cout << endl;
    cout << t.do_something() << endl;
    cout << endl;
     
    return 0;
}
